{
  "name": "KBS_Radio_Kong",
  "version": "1.0.0",
  "description": "PC Kong radio application",
  "main": "dist/js/main/index.js",
  "scripts": {
    "clean": "rimraf .tmp dist release",
    "test": "eslint src/**/*.js src/**/*.jsx",
    "compile": "babel --out-dir .tmp src",
    "watch": "babel --watch --out-dir .tmp src",
    "start": "electron .",
    "copy:resources": "cpx \"prod_resources/**/*\" dist",
    "copy:photon": "cpx \"node_modules/photon/dist/css/**/*\" dist/css && cpx \"node_modules/photon/dist/fonts/**/*\" dist/fonts",
    "prebundle": "npm run copy:resources && npm run copy:photon",
    "bundle": "webpack",
    "package": "npm run bundle && electron-packager dist \"KBS_Radio_Kong\" --all --out=final --icon=\"icon/icon\""
  },
  "author": "Yoojin Ha",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "electron": "^1.6.1",
    "electron-packager": "^8.5.2",
    "moment": "^2.21.0",
    "photon": "github:connors/photon",
    "react-day-picker": "^7.1.2",
    "style-loader": "^0.13.1",
    "video.js": "^6.6.3",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "@google-cloud/firestore": "^0.13.0",
    "axios": "^0.17.1",
    "firebase": "^3.6.4",
    "firebase-admin": "^5.10.0",
    "firebase-functions": "^0.8.1",
    "hls.js": "^0.8.9",
    "lodash": "^4.17.5",
    "react": "^15.4.2",
    "react-datepicker": "^1.2.2",
    "react-dom": "^15.4.2",
    "react-http-request": "^1.0.4",
    "react-images": "^0.5.16",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "semantic-ui-react": "^0.78.3",
    "video-react": "^0.9.4"
  }
}
